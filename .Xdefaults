! Xft
Xft.dpi:                    96
Xft.antialias:              true

!Fonts
URxvt.geometry:         90x30
URxvt*font:             xft:Hack:pixelsize=16:antialias=true:hinting=true
URxvt.saveLines:        12000
URxvt*letterSpace: -1

URxvt.scrollBar:        false
*cursorBlink:           false

! special
*.foreground:   #a6a28c
*.background:   #181818
*.cursorColor:  #a6a28c

! black
*.color0:       #20201d
*.color8:       #7d7a68

! red
*.color1:       #045a09
*.color9:       #0a6118

! green
*.color2:       #60ac39
*.color10:      #60ac39

! yellow
*.color3:       #cfb017
*.color11:      #cfb017

! blue
*.color4:       #187a2a
*.color12:      #04670f

! magenta
*.color5:       #b854d4
*.color13:      #b854d4

! cyan
*.color6:       #1fad83
*.color14:      #1fad83

! white
*.color7:       #a6a28c
*.color15:      #fefbec
